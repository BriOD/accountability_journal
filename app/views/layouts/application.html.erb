<!DOCTYPE html>
<html>
  <head>
    <title>AccountabilityJournal</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <% if current_user %>
    Signed in as <%= current_user.name %> | <%= link_to "Logout", logout_path, method: "delete" %>
  <% else %>
    <%= link_to 'Login', create_session_path %> | <%= link_to 'Signup', user_signup_path %>
  <% end %>

    <%= yield %>
  </body>
</html>
